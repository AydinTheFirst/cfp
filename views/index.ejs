						</li>
						<li><i class="fa-solid fa-check mx-2"></i>24/7 support</li>
						<li>
							<i class="fa-solid fa-check mx-2"></i>Making money is guaranteed
						</li>
						<li>
							<i class="fa-solid fa-check mx-2"></i>Certified millionaire
							coaches
						</li>
						<li><i class="fa-solid fa-check mx-2"></i>Supportive community</li>
					</ul>
				</div>
				<style>
					ul.list-unstyled {
						margin: 0;
						padding: 0;
					}

					ul.list-unstyled li {
						margin: 10px 0;
					}
				</style>
			</div>

			<!-- Slider -->
			<%- include("components/title.ejs", {title: "Testimonials", id:
			"testimonials"}) %> <%- include("components/home/slider.ejs") %>
			<button class="btn btn-warning btn-large w-100 btn-gold fw-bold">
				Join Now!
			</button>

			<!-- Details -->
			<%- include("components/title.ejs", {title: "Programs", id: "programs"})
			%>
			<!--  -->
			<% programs.map((d,i) => {%>
			<!--  -->
			<%- include("components/home/program.ejs", {d,i}) %>
			<!-- -->
			<% }) %>

			<!-- Details -->
			<%- include("components/title.ejs", { title: "Meet The Professors", id:
			"professors" }) %>
			<!--  -->
			<% profs.map((d,i) => {%>
			<!--  -->
			<%- include("components/home/card.ejs", {d,i}) %>
			<!-- -->
			<% }) %>

			<!-- 
				Affilate Links
			 -->
			<!-- Details -->
			<%- include("components/title.ejs", {title: "Affilate Program",
			id:"affilate"}) %>
			<!--  -->
			<%- include("components/home/affilate.ejs") %>

			<!-- Details -->
			<%- include("components/title.ejs", {title: "Videos", id:"videos"}) %>
			<!--  -->
			<div class="row">
				<% slideVidData.map((v,i) => { %>
				<!--  -->
				<div
					class="col-12 d-flex justify-content-center"
					style="
						position: sticky;
						top: 50px;
					">
					<video
						src="<%= v %>"
						class="rounded"
						controls
						style="object-fit: contain; width: 300px"></video>
				</div>

				<% }) %>
			</div>
		</div>

		<!-- Footer -->
		<%- include("components/footer.ejs") %>
	</body>
</html>
